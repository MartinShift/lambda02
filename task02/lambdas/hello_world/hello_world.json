{
    "lambda_path": "lambdas/hello_world",
    "name": "hello_world",
    "runtime": "java11",
    "handler": "com.task02.HelloWorld::handleRequest",
    "alias": "${lambdas_alias_name}",
    "publish_version": true,
    "url_config": {
      "authorization_type": "NONE",
      "cors": {
        "allow_origins": ["*"],
        "allow_methods": ["*"],
        "allow_headers": ["*"],
        "allow_credentials": true,
        "max_age": 300
      }
    }
  }